 public static final float WORLD_WIDTH = 1000.0;
 public static final float WORLD_HEIGHT = 1000.0;
 
 public static int getLoc(float x, float y){
   if(x >= WORLD_WIDTH || x <= 0 || y >= WORLD_HEIGHT || y <= 0) return -1;

   return world[ceil(32*x/WORLD_WIDTH)-1][ceil(32*y/WORLD_HEIGHT)-1]; 
   
 }
